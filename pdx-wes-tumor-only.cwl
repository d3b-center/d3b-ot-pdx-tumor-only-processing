        separate: true
        itemSeparator: ','
        sbg:cmdInclude: true
      description: Annotate using a list of INFO fields (list is a comma separated
        list of fields).
      sbg:category: Command options
      sbg:toolDefaultValue: All fields
    - id: '#id_field'
      label: Only annotate ID field
