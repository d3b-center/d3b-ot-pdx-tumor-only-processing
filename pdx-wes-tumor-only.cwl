      - boolean
      inputBinding:
        prefix: -noAlt
        position: 10
        separate: true
        sbg:cmdInclude: true
      description: |-
        Do not use REF and ALT fields when comparing database.vcf entries to file.vcf entries.
      sbg:category: Command options
      sbg:toolDefaultValue: 'False'
