    - class: sbg:CPURequirement
      value: 1
    - class: sbg:MemRequirement
      value:
        class: Expression
        engine: '#cwl-js-engine'
        script: |-
          {
            if ($job.inputs.total_memory)
            {
              return $job.inputs.total_memory * 1024
            }
            
            else
            {
              return 8192
            }
          }
